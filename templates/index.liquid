    <section> <!-- START: make a wish   -->

        <!-- ============================================== -->
        <!-- !                MAKE A WISH                   -->
        <!-- ============================================== -->

        <div class="custom-panel white" id="sec_6">
            <div class="row make-a-wish_logo">
